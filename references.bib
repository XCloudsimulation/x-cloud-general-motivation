	Keywords = {cloud computing;embedded systems;service-oriented architecture;telecommunication computing;wireless sensor networks;ICT infrastructure;NaaS;Smart Cities concept;WSN cloud;WSN infrastructure;WSN maintenance;WSN system operation;WSN-SOrA approach;WSN-service orchestration architecture;backend system;cloud ready infrastructure;cost reduction;embedded system;information and communication technology;network-as-a-service;on-demand provisioning;service orientated software architecture;service provisioning;wireless sensor network;Cloud computing;Computer architecture;Energy management;Engines;Logic gates;Service oriented architecture;Wireless sensor networks;IaaS;Service Orchestration;WSN Cloud},
	Pages = {962-969},
