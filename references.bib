@InProceedings{5982625,
  Title                    = {Mobile cloud computing service based on heterogeneous wireless and mobile P2P networks},
  Author                   = {Larosa, Y.T. and Jiann-Liang Chen and Der-Jiunn Deng and Han-Chieh Chao},
