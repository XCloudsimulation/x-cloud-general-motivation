@Article{1413185,
  Title                    = {Information raining and optimal link-layer design for mobile hotspots},
  Author                   = {Ho, D.H. and Valaee, S.},
  Journal                  = {Mobile Computing, IEEE Transactions on},
  Year                     = {2005},

  Month                    = {May},
  Number                   = {3},
  Pages                    = {271-284},
  Volume                   = {4},

  Abstract                 = {In this paper, we propose a link layer design for mobile hotspots. We design a novel system architecture that enables high-speed Internet access in railway systems. The proposed design uses a number of repeaters placed along the track and multiple antennas installed on the roof of a vehicle. Each packet is decomposed into smaller fragments and relayed to the vehicle via adjacent repeaters. We also use erasure coding to add parity fragments to original data. This approach is called information raining since fragments are rained upon the vehicle from adjacent repeaters. We investigate two instances of information raining. In blind information raining, all repeaters awaken when they sense the presence of the vehicle. The fragments are then blindly transmitted via awakened repeaters. A vehicle station installed inside the train is responsible for aggregating a large enough number of fragments. In the throughput-optimized information raining, the vehicle station selects a bipartite matching between repeaters and roof-top antennas and activates only a subset of the repeaters. It also dictates the amount of transmission power of each activated repeater. Both the bipartite matching and power allocations are individually shown to be NP-complete. Matching heuristics based on the Hungarian algorithm and Gale-Shapley algorithm are proposed. A simplex-type algorithm is proposed as the power allocation heuristics.},
  Doi                      = {10.1109/TMC.2005.42},
  ISSN                     = {1536-1233},
  Keywords                 = {Internet;computational complexity;data communication;encoding;land mobile radio;mobile computing;railways;road vehicles;Gale-Shapley algorithm;Hungarian algorithm;NP-complete matching;bipartite matching;blind information raining;erasure coding;high-speed Internet access;matching heuristics;mobile hotspots;optimal link-layer design;packet decomposition;packet relay;parity fragments;power allocation heuristics;power allocations;railway systems;simplex-type algorithm;system architecture;throughput-optimized information raining;Bit error rate;Internet;Low earth orbit satellites;Microcell networks;Rail transportation;Repeaters;Telecommunication traffic;Vehicles;Virtual private networks;Wireless LAN;Index Terms- Emerging technologies;combinatorial algorithms.;constrained optimization;graph theory;graphs and networks;linear programming;medium access control;mobile communication systems;mobile environments;mobile hotspots;network architecture and design;network protocols;wireless communication}
